{
  "name": "audio auto play",
  "version": "1.0",
  "description": "play audio automatically",
  "manifest_version": 2,

  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "*://*/*"],

  "icons": {
      "16": "images/icon16.png"
  }
}
